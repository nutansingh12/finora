{
  "version": 2,
  "name": "finora-web",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "http://localhost:3001/api/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://finora-backend.vercel.app",
    "NEXT_PUBLIC_APP_VERSION": "1.0.0"
  },
  "functions": {
    "packages/web/pages/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  }
}
