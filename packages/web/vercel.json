{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://finora-backend.vercel.app/api/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://finora-backend.vercel.app",
    "NEXT_PUBLIC_APP_VERSION": "1.0.0"
  }
}
